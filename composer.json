{
    "name": "leyo/mpesa",
    "description": "Mpesa G2 Api Laravel Negotiator",
    "type": "Mobileintegration",
    "license": "MIT",
    "authors": [
        {
            "name": "Le-Yo",
            "email": "lenykoskey@yahoo.com"
        }
    ],
    "keywords": [
    "MPESA",
    "Larvel",
    "G2 API",
    "REST"
    ],
    "classmap": [
        "src/Http/controllers",
        "src/Http/migrations",
        "src/Http/seeds",
        "src/Http/models",
        "src/Http/Middleware",
        "src/Http/Configs"
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "leyo\\rapidussd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "leyo\\rapidussd\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable",
    "require": {}
}
